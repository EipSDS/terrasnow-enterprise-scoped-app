        <value>//get the aws-account-info endpoint&#13;
var aws_account_info_endpoint = "";&#13;
var gr = new GlideRecord('x_terraform_snow_terrasnow_rest_endpoints');&#13;
gr.addQuery("name", "aws-account-info");&#13;
gr.query();&#13;
if (gr.next()){&#13;
    aws_account_info_endpoint = gr.full_url;&#13;
}&#13;
&#13;
&#13;
var target = new GlideRecord('x_terraform_snow_cloud_account_details');&#13;
target.query();&#13;
while (target.next()){&#13;
setIt(makeIt(target.aws_region, target.aws_role, getIt));&#13;
}&#13;
&#13;
function makeIt(region, arn, callback) {&#13;
    var requestBody = {};&#13;
    var requestData = {};&#13;
    var requestDataList = [];&#13;
&#13;
    requestData.region = region.toString();&#13;
    requestData.arn = arn.toString();&#13;
    requestData.duration = "900";&#13;
&#13;
    requestDataList[0] = requestData;&#13;
    requestBody["data"] = requestDataList;&#13;
    return callback(JSON.stringify(requestBody));&#13;
}&#13;
&#13;
function getIt(requestBody) {&#13;
    var request  = new sn_ws.RESTMessageV2();&#13;
    request.setHttpMethod('post');&#13;
    request.setEndpoint(aws_account_info_endpoint);&#13;
    request.setMIDServer(workflow.scratchpad.mid_server);&#13;
    request.setRequestBody(requestBody);&#13;
    var response = request.executeAsync();&#13;
    response.waitForResponse(60);&#13;
    return response;&#13;
}&#13;
&#13;
function setIt(response) {&#13;
    if (response.getErrorMessage() == null | response.getErrorMessage() == '') {&#13;
      var json_obj = JSON.parse(response.getBody());&#13;
      var gdt = new GlideDateTime();&#13;
      target.aws_account_info_last_updated = gdt.getDisplayValue(); //set last time last updated&#13;
      target.amis = JSON.stringify(json_obj['amis']);&#13;
      target.security_groups = JSON.stringify(json_obj['security_groups']);&#13;
      target.key_pairs = JSON.stringify(json_obj['key_pairs']);&#13;
      target.subnets = JSON.stringify(json_obj['subnets']);&#13;
      target.update();&#13;
    }&#13;
    else{&#13;
      gs.info(JSON.stringify(response.getBody()));&#13;
      gs.info('ERROR');&#13;
    }&#13;
&#13;
//gs.info(response.getBody())&#13;
//gs.info(JSON.stringify(response));&#13;
}&#13;
</value>
